{
    "name": "ecentria/ecentria-rest-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "description": "Goal of this bundle is to simplify process of creating APIs with Symfony. We use already well-coded libraries, combine them together to simplify process and not to re-invent the wheel. We've chose REST and HATEOS to have unified standards of API requests and responses.",
    "keywords": ["ecentria", "library", "rest"],
    "homepage": "https://github.com/chernecov/EcentriaRestBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Chernecov",
            "email": "sergey.chernecov@intexsys.lv"
        },
        {
            "name": "Ruslan Zavacky",
            "email": "ruslan.zavacky@intexsys.lv"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/chernecov/EcentriaRestBundle"
        }
    ],
    "require": {
        "php": ">7.0.0",
        "symfony/symfony": "~3.0",
        "friendsofsymfony/rest-bundle": "~2.0",
        "jms/serializer-bundle": "~1.1",
        "willdurand/rest-extra-bundle": "~1.0",
        "willdurand/hateoas-bundle": "~1.2.0",
        "gedmo/doctrine-extensions": "~2.4.20",
        "egulias/email-validator": "~1.2",
        "nelmio/api-doc-bundle": "~2.13.0",
        "doctrine/doctrine-bundle": "~1.5",
        "sensio/framework-extra-bundle": "~3.0.16",
        "stof/doctrine-extensions-bundle": "~1.2.2"
    },
    "autoload": {
        "psr-4": { "Ecentria\\Libraries\\EcentriaRestBundle\\": "" }
    },
    "branch-alias": {
        "dev-master": "0.0.x-dev"
    }
}
